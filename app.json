  "repository": "https://github.com/terminalh2t3/ebiz-shop-bot",
  "logo": "https://www.marketingcloud.com/sites/exacttarget/files/partner-logos/e-biz_solutions_-_logo.png",
  "keywords": ["php", "Giga.ai", "Heroku Connect"],
